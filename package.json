{
  "name": "markdown-svelte",
  "displayName": "Markdown Svelte Syntax Highlight",
  "description": "Adds Svelte syntax highlighting in Markdown and MDX code blocks",
  "version": "1.0.0",
  "publisher": "itslooklike",
  "repository": {
    "url": "https://github.com/itslooklike/vscode-ext-markdown-svelte.git"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "svelte",
    "highlight",
    "markdown",
    "mdx"
  ],
  "icon": "icons/svelte-md-logo.png",
  "contributes": {
    "grammars": [
      {
        "scopeName": "markdown.svelte.codeblock",
        "path": "./syntaxes/codeblock.json",
        "injectTo": [
          "text.html.markdown",
          "source.mdx"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.svelte": "svelte"
        }
      }
    ]
  }
}
